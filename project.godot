[display]

window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/dice_syntax/plugin.cfg", "res://addons/ColorConsole/plugin.cfg")

